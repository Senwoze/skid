local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Senwoze/skid/main/lolilol"))() 
local venyx = library.new("Demonology")

local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local RunService = game:GetService("RunService")

queueteleport = (syn and syn.queue_on_teleport) or queue_on_teleport or (fluxus and fluxus.queue_on_teleport)

local localPlayer = Players.LocalPlayer

local PlayerPage = venyx:addPage("Player")
local PlayerSection = PlayerPage:addSection("Local")
local bPlayerSection = false
local bPlayerSection1 = false

local jumpConnection
local humanoid -- Variable pour le humanoïde du joueur

-- Fonction appelée chaque fois que le personnage du joueur est ajouté (respawn)
local function onCharacterAdded(character)
    -- Attendre que le humanoïde soit disponible
    humanoid = character:WaitForChild("Humanoid")
end

-- Surveiller quand le personnage du joueur est ajouté (respawn)
localPlayer.CharacterAdded:Connect(onCharacterAdded)

-- Tentative de trouver le modèle "Ghost" dans l'espace de travail
local ghost = workspace:FindFirstChild("Ghost")

-- Valeurs par défaut au cas où le modèle Ghost n'est pas trouvé
local gender = "Unknown"
local ghostType = "Unknown"
local age = "Unknown"

-- Si le modèle Ghost est trouvé, obtenir ses attributs
if ghost then
    gender = ghost:GetAttribute("Gender") or "Unknown"
    ghostType = ghost:GetAttribute("GhostType") or "Unknown"
    age = ghost:GetAttribute("Age") or "Unknown"
end

-- Ajouter des champs texte pour afficher les détails du Ghost dans la section Player
PlayerSection:addTextbox("Ghost Gender", gender, function(value, focused)
    -- Logique de rappel (si nécessaire)
end)

PlayerSection:addTextbox("Ghost Type", ghostType, function(value, focused)
    -- Logique de rappel (si nécessaire)
end)

PlayerSection:addTextbox("Ghost Age", age, function(value, focused)
    -- Logique de rappel (si nécessaire)
end)

local VisualPage = venyx:addPage("Visual")
local walkSpeedSection = VisualPage:addSection("Universal")

local MiscellaneousPage = venyx:addPage("Misc")
local walkSpeedSection = MiscellaneousPage:addSection("Universal")

local SettingsPage = venyx:addPage("Settings")
local SettingsSection = SettingsPage:addSection("Settings")

local autoLoadEnabled = true
SettingsSection:addToggle("Auto Load", autoLoadEnabled, function(value)
    autoLoadEnabled = value
    
    if autoLoadEnabled == true then
        Players.LocalPlayer.OnTeleport:Connect(function(State)
            if queueteleport then
                queueteleport("loadstring(game:HttpGet('https://raw.githubusercontent.com/Senwoze/skid/main/mdrmdr'))()")
            end
        end)
    else
        --
    end
end)

-- Fonction pour ajouter un raccourci clavier
function AddKeybind(keyCode, action)
    local input = game:GetService("UserInputService")

    input.InputBegan:Connect(function(key, proc)
        if key.KeyCode == keyCode and not proc then
            action()
        end
    end)
end

AddKeybind(Enum.KeyCode.Insert, function()
    venyx:toggle()
end)

venyx:SelectPage(PlayerPage, true)
venyx:Notify("v1.0", "Demonology successfully loaded")
